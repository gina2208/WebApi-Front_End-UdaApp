<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebApi_Front_End_UdaApp</title>
    <link rel="icon" href="~/favicon.ico" />
    <meta name="description" content="Descripción del sitio o página actual" />
    <meta name="author" content="Tu nombre o empresa" />
</head>
<body>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <script asp-src="lib/jquery/dist/jquery.min.js"></script>
    <script asp-src="lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script asp-src="js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
